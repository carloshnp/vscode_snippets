{
  "Limit": {
    "prefix": ["lim", "\\lim"],
    "body": ["\\lim_{$1} $2"],
    "description": "Limit of a function."
  },
  "Partial derivative": {
    "prefix": ["part"],
    "body": ["\\frac{\\partial ${1:V}}{\\partial ${2:x}} $0"],
    "description": "Partial derivative of a function."
  },
  "Derivative": {
    "prefix": ["ddx"],
    "body": ["\\frac{\\mathrm{d/${1:V}}}{\\mathrm{d${2:x}}} $0"],
    "description": "Derivative of a function."
  },
  {
    "Left Right parenthesis": {
      "prefix": ["leftright"],
      "body": ["\\left( $0 \\right)"],
      "description": "Adds a parenthesis that folds the equation in display style"
    }
  }
}
